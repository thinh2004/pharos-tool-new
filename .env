# Pharos Auto Bot Configuration
# Copy this to .env and fill in your private keys

# Private Keys (add as many as needed) bắt buộc nhập đủ 3 key, nếu nhiều hơn thì add thêm
PRIVATE_KEY_1="key01"
PRIVATE_KEY_2="key02" 
PRIVATE_KEY_3="key03"

# RPC Configuration
RPC_URL="https://testnet.dplabs-internal.com"
CHAIN_ID=688688

# Bot Settings
DELAY_BETWEEN_ACCOUNTS=5000
DELAY_BETWEEN_TASKS=2000
DELAY_BETWEEN_CYCLES=1800000

# RWAfi Settings
RWAFI_ENABLED=true
STAKING_AMOUNT="0.01"
BRIDGE_AMOUNT="0.005"
CFD_LEVERAGE=2
NFT_BUDGET="0.02"

# Proxy Settings
USE_PROXY=false
